          <span class="nt">&lt;tr&gt;&lt;td&gt;</span>#{a.name}<span class="nt">&lt;/td&gt;&lt;td&gt;</span>#{a.description}<span class="nt">&lt;/td&gt;&lt;/tr&gt;</span>
